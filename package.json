{
  "name": "createjs-basic-scrollbar",
  "version": "0.3.1",
  "description": "Scrollbar library for CreateJS.",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/createjs-basic-scrollbar.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "keywords": [
    "createjs"
  ],
  "dependencies": {
    "@types/easeljs": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@createjs/easeljs": "^2.0.0-beta.3",
    "@types/jest": "^23.3.3",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc && npm run copy && npm run remove && npm run prettier:src && npm run babel",
    "prettier:src": "prettier --write './!(bin)/**/*.{js,ts,json}' ",
    "server": "http-server ./example/ -o",
    "copy": "cpx './bin/src/*' ./bin",
    "remove": "rimraf ./bin/__test__ ./bin/src",
    "babel": "webpack --mode development --module-bind js=babel-loader ./example/main.js --output ./example/bundle.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  }
}
